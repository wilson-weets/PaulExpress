{
  "name": "paulexpress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "run-s tsc:quiet"
    }
  },
  "watch": {
    "gen-locales": "src/locales/ETLCodes.ts"
  },
  "scripts": {
    "start": "run-p start:web watch",
    "start:web": "parcel serve ./src/index.html",
    "tsc": "tsc",
    "tsc:quiet": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src/**/*.{ts,tsx} --quiet --fix ",
    "gen-locales": "ts-node --project ./scripts/tsconfig.json ./scripts/generateLocales.ts",
    "gen-api": "openapi-generator generate -i http://localhost:5000/swagger/v1.0/swagger.json -g typescript-fetch -o ./src/api -c ./apiGeneratorConfig.json --skip-validate-spec",
    "prettier": "prettier ./src/**/*.{ts,tsx} --write --list-different",
    "watch": "npm-watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@blueprintjs/core": "^3.19.1",
    "@blueprintjs/datetime": "^3.15.1",
    "@blueprintjs/select": "^3.11.2",
    "@blueprintjs/table": "^3.8.2",
    "@reduxjs/toolkit": "^1.0.4",
    "classnames": "^2.2.6",
    "formik": "^2.0.4",
    "i18next": "^19.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nsitools-react": "^1.3.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.2.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "styled-components": "^4.4.1",
    "typescript": "^3.7.2",
    "use-react-router": "^1.0.7",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.7-4.2.1",
    "@types/classnames": "^2.2.9",
    "@types/eslint": "^6.1.3",
    "@types/lodash": "^4.14.149",
    "@types/parcel-bundler": "^1.12.1",
    "@types/prettier": "^1.18.4",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-virtualized": "^9.21.5",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^4.4.0",
    "@types/yup": "^0.26.24",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.2"
  }
}
